<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="fill_parent"
    android:background="@color/black" 
   
    >

     <ScrollView 
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:id="@+id/scrollView1" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        android:layout_marginTop="60dp"
         android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin" >
    
   <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:gravity="center_vertical"
       android:orientation="vertical"
       tools:ignore="UselessParent" >

    <ImageView
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:layout_gravity="center_horizontal"
        android:contentDescription="@string/todo"
        android:onClick="qpca"
        android:clickable="true"
        android:src="@drawable/ca"
         />
    
    <ImageView
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:layout_gravity="center_horizontal"
       
        android:onClick="qpcn"
        android:clickable="true"
        android:src="@drawable/cn" android:contentDescription="@string/todo"/>
    
    <ImageView
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:layout_gravity="center_horizontal"
       
        android:onClick="qpvlsi"
        android:clickable="true"
        android:src="@drawable/vlsi" android:contentDescription="@string/todo"/>
    
    
    <ImageView
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:layout_gravity="center_horizontal"
        android:contentDescription="@string/todo"
        android:onClick="qpawp"
        android:clickable="true"
        android:src="@drawable/awp"
         />
    
    <ImageView
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:layout_gravity="center_horizontal"
        android:contentDescription="@string/todo"
        android:onClick="qppom"
        android:clickable="true"
        android:src="@drawable/pom"
         />
    
    <ImageView
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:layout_gravity="center_horizontal"
        android:contentDescription="@string/todo"
        android:onClick="qpme"
        android:clickable="true"
        android:src="@drawable/me"
         />
    
     <ImageView
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:layout_gravity="center_horizontal"
        android:contentDescription="@string/todo"
        android:onClick="qpadsp"
        android:clickable="true"
        android:src="@drawable/adsp"
         />
     
      <ImageView
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:layout_gravity="center_horizontal"
        android:contentDescription="@string/todo"
        android:onClick="qpos"
        android:clickable="true"
        android:src="@drawable/os"
         />
      
      <ImageView
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="50dp"
        android:contentDescription="@string/todo"
        android:onClick="qpra"
        android:clickable="true"
        android:src="@drawable/ra"
         />
    

    
    
    

</LinearLayout>

   
   
</RelativeLayout>
  </ScrollView> 

    <TextView
		    android:id="@+id/textView1"
		    android:layout_width="fill_parent"
		    android:layout_height="50dp"
		    android:layout_gravity="top"
		    android:gravity="center_horizontal"
		    android:background="@color/ibg"
		    android:fontFamily="sans-serif-condensed"
		    
		   
		    android:paddingTop="15dp"
		    android:text="@string/title_activity_sixth_semester"
		    android:textColor="@color/white"
		    android:textSize="20sp"
		    tools:ignore="UnusedAttribute,RtlSymmetry" />
      

</FrameLayout>